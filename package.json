{
  "name": "attajak-doks",
  "version": "0.1.0",
  "description": "Hugo + Doks theme",
  "author": "Attajak",
  "license": "MIT",
  "scripts": {
    "create": "hugo new content",
    "dev": "hugo server --disableFastRender --noHTTPCache --logLevel info -b http://localhost/ -O",
    "format": "prettier **/** -w -c",
    "build": "hugo --minify --gc --logLevel info",
    "preview": "vite preview --outDir public"
  },
  "dependencies": {
    "@tabler/icons": "^3.24.0",
    "@thulite/doks-core": "^1.8.0",
    "@thulite/images": "^3.3.0",
    "@thulite/inline-svg": "^1.2.0",
    "@thulite/seo": "^2.4.1",
    "thulite": "^2.5.0"
  },
  "devDependencies": {
    "prettier": "^3.4.2",
    "vite": "^5.4.11"
  },
  "engines": {
    "node": ">=20.11.0"
  }
}
